	<xs:simpleType name="UUID32">
		<xs:annotation>
			<xs:documentation>UUID без разделителей. Набор из 32 символов [0-9], [a-f] или [A-F], без разделителей
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9a-fA-F]){32}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CardNumType">
		<xs:annotation>
			<xs:documentation>Номер карты без разделителей. Набор из 16 символов [0-9]</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]){16}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountNumType">
		<xs:annotation>
			<xs:documentation>Номер счета без разделителей. Набор из 20 символов [0-9]</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="20"/>
			<xs:pattern value="[0-9]{20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="INNType">
		<xs:annotation>
			<xs:documentation>Номер ИНН. Набор символов [0-9], Min 10 Max 12</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="10"/>
			<xs:maxLength value="12"/>
			<xs:pattern value="([0-9]){10,12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BICType">
		<xs:annotation>
			<xs:documentation>Номер БИК. Набор из 9 символов [0-9]</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="9"/>
			<xs:pattern value="([0-9]){9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OGRNType">
		<xs:annotation>
			<xs:documentation>Номер ОГРН. Набор символов [0-9], Min 13 Max 15</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="13"/>
			<xs:maxLength value="15"/>
			<xs:pattern value="([0-9]){13,15}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KPPType">
		<xs:annotation>
			<xs:documentation>Номер КПП. Набор из 9 символов [0-9]</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="PaymentQueueType">
		<xs:annotation>
			<xs:documentation>Очередность платежа. Символы (римские цифры от I до V)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="I"/>
			<xs:enumeration value="II"/>
			<xs:enumeration value="III"/>
			<xs:enumeration value="IV"/>
			<xs:enumeration value="V"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PkgStatusType">
		<xs:annotation>
			<xs:documentation>Код статуса пакета: New, Packed, Completed</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="New"/>
			<xs:enumeration value="Packed"/>
			<xs:enumeration value="Completed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PkgTaskStatusType">
		<xs:annotation>
			<xs:documentation>Код статуса задачи в пакете: Active, OnApproval, Declined, Approved, Forwarded</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Active"/>
			<xs:enumeration value="OnApproval"/>
			<xs:enumeration value="Declined"/>
			<xs:enumeration value="Approved"/>
			<xs:enumeration value="Forwarded"/>
			<xs:enumeration value="OnSecHandApproval"/>
			<xs:enumeration value="SecHandApproved"/>
			<xs:enumeration value="SecHandDeclined"/>
			<xs:enumeration value="Completed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OVNStatusType">
		<xs:annotation>
			<xs:documentation>Статус ОВН: New, Pending, Completed, Deleted</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="New"/>
			<xs:enumeration value="Pending"/>
			<xs:enumeration value="Completed"/>
			<xs:enumeration value="Deleted"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CopType">
		<xs:annotation>
			<xs:documentation>Сумма, коп.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minExclusive value="00"/>
			<xs:maxExclusive value="99"/>
			<xs:totalDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperTypeLabel">
		<xs:annotation>
			<xs:documentation>Тип операции</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ToCardDeposit"/>
			<xs:enumeration value="FromCardWithdraw"/>
			<xs:enumeration value="ToAccountDepositRub"/>
			<xs:enumeration value="FromAccountWithdrawRub"/>
			<xs:enumeration value="CheckbookIssuing"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CashSymbolType">
		<xs:annotation>
			<xs:documentation>Кассовый символ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(0-9){2,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IDDtype">
		<xs:annotation>
			<xs:documentation>Тип ДУЛ: passport, internpassport, militaryID, seamenId</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="passport"/>
			<xs:enumeration value="internpassport"/>
			<xs:enumeration value="militaryID"/>
			<xs:enumeration value="seamenId"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="cbRequestStatusType">
		<xs:annotation>
			<xs:documentation>Статус заявления на выдачу чековых книжек</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="New"/>
			<xs:enumeration value="Pending"/>
			<xs:enumeration value="Ready"/>
			<xs:enumeration value="Completed"/>
			<xs:enumeration value="Deleted"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountType">
		<xs:annotation>
			<xs:documentation>Тип счета: расчётный (01), бюджетный (03),  транзитный   (04),  ссудный (05)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value=""/>
			<xs:enumeration value="01"/>
			<xs:enumeration value="03"/>
			<xs:enumeration value="04"/>
			<xs:enumeration value="05"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
