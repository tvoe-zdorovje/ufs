	<xs:simpleType name="UUID32">
		<xs:annotation>
			<xs:documentation>UUID без разделителей. Набор из 32 символов [0-9], [a-f] или [A-F], без разделителей
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9a-fA-F]){32}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperTypeLabel">
		<xs:annotation>
			<xs:documentation>Тип операции</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ToCardDeposit"/>
			<xs:enumeration value="FromCardWithdraw"/>
			<xs:enumeration value="ToAccountDepositRub"/>
			<xs:enumeration value="FromAccountWithdrawRub"/>
			<xs:enumeration value="CheckbookIssuing"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CashOrderStatusType">
		<xs:annotation>
			<xs:documentation>Статус кассового ордера: Created, Committed</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Created"/>
			<xs:enumeration value="Committed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="INNType">
		<xs:annotation>
			<xs:documentation>Номер ИНН. Набор символов [0-9], Min 10 Max 12</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="10"/>
			<xs:maxLength value="12"/>
			<xs:pattern value="([0-9]){10,12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountNumType">
		<xs:annotation>
			<xs:documentation>Номер счета без разделителей. Набор из 20 символов [0-9]</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="20"/>
			<xs:pattern value="[0-9]{20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BICType">
		<xs:annotation>
			<xs:documentation>Номер БИК. Набор из 9 символов [0-9]</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="9"/>
			<xs:pattern value="([0-9]){9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OGRNType">
		<xs:annotation>
			<xs:documentation>Номер ОГРН. Набор символов [0-9], Min 13 Max 15</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="13"/>
			<xs:maxLength value="15"/>
			<xs:pattern value="([0-9])"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CashSymbolType">
		<xs:annotation>
			<xs:documentation>Кассовый символ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(0-9){2,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KPPType">
		<xs:annotation>
			<xs:documentation>Номер КПП. Набор из 9 символов [0-9]</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperCatType">
		<xs:annotation>
			<xs:documentation>Категория операции 0 - расходная, 1 - приходная, 2 - не применимо</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:pattern value=""/>
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IDDtype">
		<xs:annotation>
			<xs:documentation>Тип ДУЛ: passport, internpassport, militaryID, seamenId</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="passport"/>
			<xs:enumeration value="internpassport"/>
			<xs:enumeration value="militaryID"/>
			<xs:enumeration value="seamenId"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountType">
		<xs:annotation>
			<xs:documentation>Тип счета: расчётный (01), бюджетный (03),  транзитный   (04),  ссудный (05)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value=""/>
			<xs:enumeration value="01"/>
			<xs:enumeration value="03"/>
			<xs:enumeration value="04"/>
			<xs:enumeration value="05"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OpStatusType">
		<xs:annotation>
			<xs:documentation>Статус операции(транзакции): New, AdvanceReservation, Committed, Cancelled</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="New"/>
			<xs:enumeration value="AdvanceReservation"/>
			<xs:enumeration value="Committed"/>
			<xs:enumeration value="Cancelled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CashOrderType">
		<xs:annotation>
			<xs:documentation>Тип кассового ордера. 'KO1' - приходный , 'KO2' - расходный</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="KO1"/>
			<xs:enumeration value="KO2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LimitStatusType">
		<xs:annotation>
			<xs:documentation>Статус проверки лимитов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="LIMIT_ERROR"/>
			<xs:enumeration value="LIMIT_PASSED"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
